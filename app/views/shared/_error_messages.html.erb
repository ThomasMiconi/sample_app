<% if @myuser.errors.any? %> <div id="error_explanation">
        <div class="alert alert-error">
            The form contains <%= pluralize(@myuser.errors.count, "error") %>.
        </div>
        <ul>
            <% @myuser.errors.full_messages.each do |msg| %>
                <li>* <%= msg %></li> <% end %>
        </ul>
    </div> <% end %>


